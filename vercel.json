{
  "version": 2,
  "builds": [
    { "src": "dist/index.html", "use": "@vercel/static" },
    { "src": "dist/assets/**", "use": "@vercel/static" },
    { "src": "dist/**/*", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "github": {
    "silent": true
  }
}